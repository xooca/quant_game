ance().clear()\n","data_config = du.initialize_config(**master_config)"],"metadata":{"id":"fTBj5mFm06NJ"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["data_config.common.endpoint"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"WbI8t4ew631H","executionInfo":{"status":"ok","timestamp":1664197840356,"user_tz":-330,"elapsed":4,"user":{"displayName":"Prabhaat Kumaarr","userId":"05182281128240086889"}},"outputId":"7b3e5ee9-1c65-4778-9eef-f4ca342856a2"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["{'base_url': 'https://www.alphavantage.co/query?'}"]},"metadata":{},"execution_count":10}]},{"cell_type":"code","source":["\"https://www.alphavantage.co/query?function=TIME_SERIES_INTRADAY&symbol=IBM&interval=5min&outputsize=full&apikey=demo\""],"metadata":{"id":"gbWB9f1o2UiR"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["data_config.common.endpoint.base_url"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"Z03Vet_N1U65","executionInfo":{"status":"ok","timestamp":1664196517887,"user_tz":-330,"elapsed":3,"user":{"displayName":"Prabhaat Kumaarr","userId":"05182281128240086889"}},"outputId":"9ba26608-a286-4c6f-8b3c-4485d561f5b1"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["{'base_url': 'https://www.alphavantage.co/query?'}"]},"metadata":{},"execution_count":5}]},{"cell_type":"code","source":[],"metadata":{"id":"eVwSiNSm15vW"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["endpoint_details = dict(data_config.common.endpoint_details)\n","sub_url = ''\n","for i,j in endpoint_details.items():\n","  if j != 'None':\n","    sub_url += f\"{i}={j}&\"\n","url = f\"{self.base_url}{sub_url}\"\n","url = url[:-1]\n","url"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":36},"id":"srw-VWIe2eX7","executionInfo":{"status":"ok","timestamp":1664196918144,"user_tz":-330,"elapsed":393,"user":{"d